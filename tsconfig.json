{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "es6",
    "moduleResolution": "node",
    "sourceMap": true,
    "target": "es6",
    "jsx": "react",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "baseUrl": "./src",
    "paths": {
      "*": [
        "*"
      ]
    }
  },
  "include": [
    "./src/**/*",
    "./cypress/integration/**/*",
    "node_modules/cypress/types/cypress-expect.d.ts"
  ]
}
